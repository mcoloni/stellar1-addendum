<%= bootstrap_form_for(@binocular) do |f| %>
  <%= f.text_field :name, label: 'Nome:' %>
  <%= f.text_field :brand,label: 'Brand:' %>
  <%= f.text_area :description, label: 'Descrizione:' %>
  <%= f.number_field :magnifications, label: 'Ingrandimenti:' %>
  <%= f.text_area :notes, label: 'Note:' %>
  <%= f.submit 'Aggiorna' %>
<% end %>
